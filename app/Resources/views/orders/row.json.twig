<tr id="order_{{ order.id }}">
        <td><a href='{{ path('orders_show', { 'id': order.id }) }}'>{{ order.id }}</a></td>
        <td>{{ order.userId }}</td>
        <td>{{ order.sum }}</td>
        <td>
            <a href='{{ path('orders_show', { 'id': order.id }) }}'>show</a>
            <a class='modal-button' data-target='#myModal'
               href='{{ path('orders_edit', { 'id': order.id }) }}'>edit</a>
        </td>
</tr>
